<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RedactProfile">

    <FrameLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/_bg__iphone_se___11_ek2"
        android:layout_width="413dp"
        android:layout_height="731dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:background="@drawable/_bg__iphone_se___11_ek2_shape" />

    <ImageView
        android:id="@+id/ellipse_6"
        android:layout_width="410dp"
        android:layout_height="311dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="490dp"
        android:contentDescription="ellipse_6"
        android:src="@drawable/ellipse_6" />

    <View
        android:id="@+id/rectangle_8"
        android:layout_width="413dp"
        android:layout_height="80dp"
        android:layout_marginStart="-5dp"
        android:layout_marginTop="0dp"
        android:background="@drawable/rectangle_8_shape" />

    <ImageView
        android:id="@+id/ellipse_7"
        android:layout_width="169dp"
        android:layout_height="60dp"
        android:layout_marginStart="91dp"
        android:layout_marginTop="9dp"
        android:contentDescription="ellipse_7"
        android:src="@drawable/ellipse_7" />

    <ImageView
        android:id="@+id/ellipse_8"
        android:layout_width="197dp"
        android:layout_height="74dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="3dp"
        android:contentDescription="ellipse_8"
        android:src="@drawable/ellipse_8" />

    <TextView
        android:id="@+id/collabhub"
        android:layout_width="167dp"
        android:layout_height="40dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="23dp"
        android:fontFamily="@font/ultra"
        android:text="CollabHub"
        android:textColor="@color/_bg__iphone_se___11_ek2_color"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/polygon_3"
        android:layout_width="79dp"
        android:layout_height="41dp"
        android:layout_marginStart="330dp"
        android:layout_marginTop="19dp"
        android:contentDescription="polygon_3"
        android:src="@drawable/polygon_3" />

    <ImageView
        android:id="@+id/polygon_3_ek1"
        android:layout_width="79dp"
        android:layout_height="41dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="19dp"
        android:contentDescription="polygon_3_ek1"
        android:src="@drawable/polygon_3_ek1"/>

        <ImageView
        android:id="@+id/background_ek3"
        android:layout_width="538dp"
        android:layout_height="321dp"
        android:layout_marginStart="-100dp"
        android:layout_marginTop="150dp"
        android:contentDescription="background_ek3"
        android:src="@drawable/img_6" />

    <Button
        android:id="@+id/login_btn"
        android:layout_width="198dp"
        android:layout_height="50dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="400dp"
        android:background="@drawable/rectangle_6_shape"
        android:fontFamily="@font/ubuntu_medium"
        android:text="Сохранить" />

        <Button
        android:id="@+id/exit"
        android:layout_width="198dp"
        android:layout_height="50dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="530dp"
        android:background="@drawable/rectangle_6_shape"
        android:fontFamily="@font/ubuntu_medium"
        android:text="Назад" />


        <EditText
            android:id="@+id/about_me"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_marginStart="90dp"
            android:layout_marginTop="170dp"
            android:fontFamily="@font/ubuntu_light"
            android:hint="Обо мне:"/>

        <EditText
            android:id="@+id/age"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_marginStart="90dp"
            android:layout_marginTop="200dp"
            android:fontFamily="@font/ubuntu_light"
            android:hint="Возраст:"/>

        <EditText
            android:id="@+id/city"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_marginStart="90dp"
            android:layout_marginTop="230dp"
            android:fontFamily="@font/ubuntu_light"
            android:hint="Город:"/>

        <EditText
            android:id="@+id/work"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_marginStart="90dp"
            android:layout_marginTop="260dp"
            android:fontFamily="@font/ubuntu_light"
            android:hint="Работа:"/>

        <EditText
            android:id="@+id/phone"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_marginStart="90dp"
            android:layout_marginTop="290dp"
            android:fontFamily="@font/ubuntu_light"
            android:hint="Телефон:"/>

        <EditText
            android:id="@+id/email"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_marginStart="90dp"
            android:layout_marginTop="320dp"
            android:fontFamily="@font/ubuntu_light"
            android:hint="Email:"/>


    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>